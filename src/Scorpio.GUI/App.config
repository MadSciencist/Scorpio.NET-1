<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="cameraConfig" type="Scorpio.GUI.Configuration.CameraConfigSection, Scorpio.GUI, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>

  <appSettings>
    <!--Gamepad-->
    <add key="gamepadUpdateFrequency" value="25" />

    <!--Rabbit MQ-->
    <add key="rabbitHost" value="localhost" />
    <add key="rabbitPort" value="5672" />
    <add key="rabbitUser" value="guest" />
    <add key="rabbitPassword" value="guest" />
    <add key="rabbitVhost" value="/" />
    <add key="rabbitExchange" value="scorpio.direct" />
    <add key="rabbitMyQueue" value="scorpio.gui" />
    <add key="rabbitMessageTTL" value="5000" />
  </appSettings>

  <cameraConfig>
    <add name="Cam 1" apiEndpoint="http:/dasdasd" />
    <add name="Cam 2" apiEndpoint="dupa" />
  </cameraConfig>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
